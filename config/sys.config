[
	{simple_ranch, [
		{tcp_port, 10001}
	]},
	
	{lager, [
		{log_root, "log"},
  		{handlers, [
			{lager_console_backend, [{level, info}]},
			{lager_file_backend, [{file, "simple_ranch.log"}, {level, debug},
				{formatter_config, [date, " ", time," [",severity,"] ",pid, " ", message, "\n"]}]},
    		{lager_file_backend, [{file, "error.log"}, {level, error}]}
  		]}
	]}
].
